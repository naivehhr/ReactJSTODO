{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "quotes": [ 1, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "arrow-spacing": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-no-bind": 0,
    "brace-style": 0,
    "semi": 0,
    "babel/object-shorthand": 0,
    "react/jsx-indent": 0,
    "jsx-quotes": 0
  },
  "extends": [
    "xo-react/space",
    "xo-space/esnext"
  ]
}
